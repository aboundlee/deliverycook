{
  "presets": [
    [
      "next/babel",
      {
	"transform-runtime": {
	    "useESModules": false
	},
	"preset-env": {
	    "targets": {
		"browsers": [">0.03%"]
	    },
	    "useBuiltIns": "entry",
	    "corejs": 2
	}
      }
    ]
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      },
      "emotion"
    ]
  ],
}


